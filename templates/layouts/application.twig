<body class="fix-header fix-sidebar card-no-border logo-center">
<div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
        <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
</div>
<div id="main-wrapper">
    {% include 'elements/header.twig' %}
    {% include 'elements/navbar.twig' %}
    <div class="page-wrapper">
        {% block pageTitle %} {#Content goes here#} {% endblock %}
        <div class="container-fluid">
            {% include 'elements/flash.twig' %}<br>
            {% block content %} {#Content goes here#} {% endblock %}
        </div>
        <footer class="footer"> Â© {{ "now" | date("Y") }} SCFL </footer>
    </div>
</div>
<script src="/assets/plugins/jquery/jquery.min.js"></script>
