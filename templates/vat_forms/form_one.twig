{% extends 'layouts/company_registration_layout.twig' %}
{% block title %} SCFL | Company Registration {% endblock %}
{% block pageTitle %}
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h3 class="text-themecolor">Value Added Tax (VAT)</h3>
        </div>
    </div>
{% endblock %}
{% block content %}
    <form class="form-horizontal form-material" method="post" action="/start/VAT">
        <div class="row">
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">User (General Information)</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-md-12">First Name</label>
                                    <input type="text"  name="user[first_name]" class="form-control form-control-line">
                                </div>
                                <div class="col-md-6">
                                    <label class="col-md-12">Last Name</label>
                                    <input type="text"  name="user[last_name]" class="form-control form-control-line">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-md-12">Mobile No.</label>
                                    <input type="text"  name="user[mobile]" class="form-control form-control-line">
                                </div>
                                <div class="col-md-6">
                                    <label class="col-md-12">Email address</label>
                                    <input type="email"  name="user[email_address]" class="form-control form-control-line">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">Owner/MD Information</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-md-12">ID Card</label>
                                    <input name="VAT[md_id_card]" type="file" class="form-control form-control-line">
                                </div>
                                <div class="col-md-6">
                                    <label class="col-md-12">Photo</label>
                                    <input name="VAT[md_photo]" type="file" class="form-control form-control-line">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">eTIN/Trade</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-md-12">Company eTIN</label>
                                    <input name="VAT[company_tin]" type="file" class="form-control form-control-line">
                                </div>
                                <div class="col-md-6">
                                    <label class="col-md-12">Trade License</label>
                                    <input name="VAT[trade_license]" type="file" class="form-control form-control-line">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">Bank & VAT</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-md-12">Bank A/C (Solvency)</label>
                                    <input name="VAT[bank_ac]" type="file" class="form-control form-control-line">
                                </div>
                                <div class="col-md-6">
                                    <label class="col-md-12">Old VAT (if any)</label>
                                    <input name="VAT[ma_association]" type="file" class="form-control form-control-line">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">Certification</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">Incorporation Certificate</label>
                                        <input name="VAT[incorporation_certificate]" type="file" class="form-control form-control-line">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">MOA and AOA</label>
                                        <input name="VAT[mod_aoa]" type="file" class="form-control form-control-line">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">NID & Income</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">NID</label>
                                        <input name="VAT[nid]" type="file" class="form-control form-control-line">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">Turn Over Income (Yearly)</label>
                                        <input name="VAT[income_yearly]" type="text" class="form-control form-control-line">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">Certification</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">Incorporation Certificate</label>
                                        <input name="VAT[incorporation_certificate]" type="file" class="form-control form-control-line">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">MOA and AOA</label>
                                        <input name="VAT[mod_aoa]" type="file" class="form-control form-control-line">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card">
                    <ul class="nav nav-tabs profile-tab" role="tablist">
                        <li class="nav-item nav-link">Date and Branch Office</li>
                    </ul>
                    <div class="card-body">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">Effective Date</label>
                                        <input name="VAT[effective_date]" type="date" class="form-control form-control-line">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12">
                                        <label class="col-md-12">Branch Office</label>
                                        <input name="VAT[branch_office]" type="text" class="form-control form-control-line">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-12">
                                <button class="btn btn-success">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>



    <div class="row">
        <div class="col-lg-4 col-xlg-3 col-md-5">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <p><b>Govt. Fees : </b></p>
                            <p>Govt. Fees = 8000/- (Fees 7000 + Book 1000) + 15%</p>
                            <p><b>And Documentation and Consultancy = 10, 000/=</b></p>
                            <p><b>Time: 15 days</b></p>
                            <p><b>Code: Sonali Bank</b></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}